    "type": "library",
    "require": {
        "rappasoft/laravel-helpers": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Karoly Nemethy",
            "email": "olythy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
